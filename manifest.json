{
    "manifest_version": 2,
    "name": "Keyboard stroke logger",
    "version": "0.1",
    "author": "Matthew Teja", 
    "browser_action": {
        
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage",
        "tabs"
    ],
    
    "content_scripts":[
        {
        "matches": 
        [
        "<all_urls>"
        ],
        "js": ["jquery-3.2.1.js", "popup.js", "main.js"],
        "css": ["popup.css"]
        }    
    ]
}
